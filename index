    <script>
     function toggleText(section) {
    let content = section.querySelector('.hidden-content');
    let button = section.querySelector('.toggle-btn');

    if (content.style.display === '' || content.style.display === 'none') {
        content.style.display = 'block';
        button.innerText = 'Voir moins';
    } else {
        content.style.display = 'none';
        button.innerText = 'Voir plus';
    }
}

        function filterSections(genre) {
            const sections = document.querySelectorAll('.section');
            sections.forEach(section => {
                const genres = section.dataset.genre.split(' ');
                if (genre === 'all' || genres.includes(genre)) {
                    section.style.display = 'block';
                } else {
                    section.style.display = 'none';
                }
            });
        }
    </script>
